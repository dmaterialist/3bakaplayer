<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<DialogPane headerText="Добавить плейлист" minHeight="200.0" minWidth="500.0" prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="player.AddPlaylistController">
   <content>
      <VBox minHeight="134.0" minWidth="500.0" prefHeight="160.0" prefWidth="500.0">
         <children>
            <HBox minHeight="30.0" minWidth="450.0" prefHeight="30.0" prefWidth="450.0">
               <children>
                  <Label prefHeight="24.0" prefWidth="115.0" text="Введите ссылку" />
                  <TextField fx:id="new_link" prefHeight="26.0" prefWidth="340.0">
                     <HBox.margin>
                        <Insets left="40.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox minHeight="30.0" minWidth="450.0" prefHeight="30.0" prefWidth="450.0">
               <children>
                  <Label minHeight="20.0" minWidth="140.0" prefHeight="20.0" prefWidth="140.0" text="Изменить название ">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="new_name" minWidth="331.0" prefHeight="26.0" prefWidth="331.0">
                     <HBox.margin>
                        <Insets left="7.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
            <ButtonBar prefHeight="43.0" prefWidth="478.0">
              <buttons>
                <Button mnemonicParsing="false" onAction="#ok" text="OK" />
                  <Button mnemonicParsing="false" onAction="#close" text="Закрыть" />
              </buttons>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </ButtonBar>
         </children>
      </VBox>
   </content>
   <header>
      <Label>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
   </header>
</DialogPane>
